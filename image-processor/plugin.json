  "keywords": ["image", "processor", "events", "backend", "example"],
  "dependencies": {
    "python": ">=3.10",
    "calvin": ">=1.0.0"
  },
  "files": {
    "include": [
      "plugin.py",
      "plugin.json"
    ],
    "exclude": [
      "*.md",
      "tests/**",
      "__pycache__/**"
    ]
  },
  "requirements": {
