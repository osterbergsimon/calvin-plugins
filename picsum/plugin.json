{
  "format_version": "1.0.0",
  "id": "picsum",
  "name": "Picsum Photos",
  "version": "1.0.0",
  "type": "image",
  "description": "Random high-quality images from Picsum Photos (no API key required)",
  "author": "Calvin Team",
  "license": "MIT",
  "homepage": "https://github.com/calvin-dashboard/calvin-plugins",
  "repository": "https://github.com/calvin-dashboard/calvin-plugins",
  "keywords": ["image", "photos", "picsum", "random"],
  "dependencies": {
    "python": ">=3.10",
    "calvin": ">=1.0.0"
  },
  "files": {
    "include": [
      "plugin.py",
      "plugin.json"
    ],
    "exclude": [
      "*.md",
      "tests/**",
      "__pycache__/**"
    ]
  },
  "requirements": {
    "restart_required": true,
    "config_required": false
  }
}

