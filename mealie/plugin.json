{
  "format_version": "1.0.0",
  "id": "mealie",
  "name": "Mealie Meal Plan",
  "version": "1.0.0",
  "type": "service",
  "description": "Display weekly meal plan from Mealie recipe manager",
  "author": "Calvin Team",
  "license": "MIT",
  "homepage": "https://github.com/calvin-dashboard/calvin-plugins",
  "repository": "https://github.com/calvin-dashboard/calvin-plugins",
  "keywords": ["mealie", "meal", "recipe", "meal-plan", "service"],
  "dependencies": {
    "python": ">=3.10",
    "calvin": ">=1.0.0"
  },
  "files": {
    "include": [
      "plugin.py",
      "plugin.json",
      "frontend/**"
    ],
    "exclude": [
      "*.md",
      "tests/**",
      "__pycache__/**"
    ]
  },
  "requirements": {
    "restart_required": true,
    "config_required": true
  }
}




